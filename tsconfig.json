{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "jsx": "react-native",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "~components": ["src/components"],
        "~components/*": ["src/components/*"],
        "~store/*": ["src/store/*"],
        "~store": ["src/store"],
        "~src/*": ["src/*"]
      }
    }
  }